// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Widget {
    id     String @id @default(cuid())
    name   String
    source String
}

model Dashboard {
    id    String @id @default(cuid())
    name  String
    icon  String
    color String
}
